tell application "System Preferences"
 activate
 set the current pane to pane id "com.apple.preference.expose"
 get the name of every anchor of pane id "com.apple.preference.expose"

 set theList to the name of every anchor of pane id "com.apple.preference.expose"
repeat with a from 1 to length of theList
    set theCurrentListItem to item a of theList
    -- Process the current list item
    display dialog theCurrentListItem & " is item " & a & " in the list."
end repeat

 --> returns: {"Main", "Spaces"}
 reveal anchor "Spaces" of pane id "com.apple.preference.expose"
end tell

